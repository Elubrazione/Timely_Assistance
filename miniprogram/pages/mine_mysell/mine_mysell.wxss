.navbar_main{ 
    display: flex; 
    flex-direction: column; 
    height: 100%; 
   } 
   
   .navbar{ 
    flex: none; 
    display: flex; 
    background: #fff; 
   } 
   .navbar .item{ 
    position: relative; 
    flex: auto; 
    text-align: center; 
    line-height: 80rpx; 
   } 
   .navbar .item.active{ 
    color: #ff8000; 
   } 
   .navbar .item.active:after{ 
    content: ""; 
    display: block; 
    position: absolute; 
    bottom: 0; 
    left: 0; 
    right: 0; 
    height: 4rpx; 
    background: #ff8000; 
   }
   
   
